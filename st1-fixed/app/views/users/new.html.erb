
    <%= form_for @user, {action: user_path(@user)} do |f| %>
      <p><%= f.label :email %> 
      <%= f.text_field :email %>
      </p>
      <p>
       <%= f.label :password %>
      <%= f.text_field :password %>
      </p>
       <p>
       <%= f.label :password_confirmation %>
      <%= f.text_field :password_confirmation %>
      </p>
      <%= f.submit %>
      <% end %>